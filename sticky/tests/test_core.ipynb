      "    'test_get_dom_widget',\n",
      "    'test_set_chart_attrs',\n",
      "    'test_update'\n",
      "]\n",
      "for test in core_tests:\n",
      "    tester = stc.TestCore(test)\n",
      "    tester.run()"
